<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical">
    <RelativeLayout
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_selectresults_xuenian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="学年"
        android:layout_below="@+id/tv_selectresults_kechengming"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:id="@+id/tv_selectresults_xueqi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="学期"
        android:layout_below="@+id/tv_selectresults_kechengming"
        android:layout_toRightOf="@+id/tv_selectresults_xuenian"
        android:layout_toEndOf="@+id/tv_selectresults_xuenian" />

    <TextView
        android:id="@+id/tv_selectresults_kechengming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="课程名jbljblblhblhblblblhblblhblblhb"
        android:maxEms="12"
        android:maxLines="1"
        android:ellipsize="end"
        android:textColor="@color/topbgcolor"
        android:textSize="18dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/tv_selectresults_kechengtype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="课程类型"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_below="@+id/tv_selectresults_kechengming"
        android:layout_toRightOf="@+id/tv_selectresults_xueqi"
        android:layout_toEndOf="@+id/tv_selectresults_xueqi" />

    <TextView
        android:id="@+id/tv_selectresults_teacher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="教师"
        android:layout_alignTop="@+id/tv_selectresults_kaohefangshi"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/tv_selectresults_kaohefangshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="考核方式"
        android:layout_marginRight="23dp"
        android:layout_marginEnd="23dp"
        android:layout_alignTop="@+id/tv_selectresults_kechengtype"
        android:layout_toLeftOf="@+id/tv_selectresults_teacher"
        android:layout_toStartOf="@+id/tv_selectresults_teacher" />
  <TextView
        android:id="@+id/tv_selectresults_zongping"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="总评成绩"
      android:textSize="18dp"
      android:textColor="@color/topbgcolor"
      android:layout_alignParentTop="true"
      android:layout_alignParentRight="true"
      android:layout_alignParentEnd="true" />
    </RelativeLayout>

</RelativeLayout>